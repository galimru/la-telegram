{
  "chat_id" : "${chat_id}",
  "text" : "Выберите подраздел форума из списка ниже:",
  "parse_mode" : "markdown",
  "reply_markup" : {
    "@class" : "com.github.galimru.telegram.objects.InlineKeyboardMarkup",
    "inline_keyboard" : [
      <#list forums as forum>
      [
        {
          "text" : "${forum.name}",
          "callback_data" : "${forum.id}"
        }
      ]<#if forum?has_next>,</#if>
      </#list>
    ]
  }
}