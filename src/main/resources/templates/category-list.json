{
  "chat_id" : "${chat_id}",
  "text" : "Выберите раздел форума из списка ниже:",
  "parse_mode" : "markdown",
  "reply_markup" : {
    "@class" : "com.github.galimru.telegram.objects.InlineKeyboardMarkup",
    "inline_keyboard" : [
      <#list categories as category>
      [
        {
          "text" : "${category.name}",
          "callback_data" : "${category.id}"
        }
      ]<#if category?has_next>,</#if>
      </#list>
    ]
  }
}